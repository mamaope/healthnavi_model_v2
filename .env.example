# HealthNavi environment example

# Security
SECRET_KEY=change-me-32-chars-min
ENCRYPTION_KEY=change-me-32-chars-min

# Database (PostgreSQL)
DB_USER=healthnavi_user
DB_PASSWORD=change-me
DB_HOST=postgres
DB_PORT=5432
DB_NAME=healthnavi_cdss

# Google Cloud / Vertex AI
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/run/secrets/gcp_service_account.json

# Azure OpenAI
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=change-me
API_VERSION=2024-02-01
DEPLOYMENT=text-embedding-3-large

# Zilliz/Milvus
MILVUS_URI=https://your-cluster.zillizcloud.com
MILVUS_TOKEN=change-me
MILVUS_COLLECTION_NAME=medical_knowledge

# AWS S3
AWS_ACCESS_KEY_ID=change-me
AWS_SECRET_ACCESS_KEY=change-me
AWS_REGION=us-east-1
AWS_S3_BUCKET=healthnavi-cdss

# Application
ENV=development
DEBUG=true
LOG_LEVEL=INFO
CORS_ORIGINS=["http://localhost:8080","http://localhost:8050"]

# Local data paths
# Path to the BNF SQLite database; defaults to backend/data/bnf_20210409.db if not set
BNF_DB_PATH=
